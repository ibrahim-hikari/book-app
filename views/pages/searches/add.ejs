<!DOCTYPE html>
<html lang="en">

<head>
    <title>Document</title>
</head>

<body>
    <% books.forEach(book => { %>
    <li>
        <div><%= book.title %></div>
        <div><%= book.author %></div>
        <div><img src=<%= book.image_url %>></div>
        <div><%= book.isbn %></div>
        <div><%= book.description %></div>
        <div><%= book.bookshelf %></div>
    </li>


    <% }) %>
</body>

</html>